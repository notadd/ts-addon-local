{
  "name": "ext-local-store",
  "version": "1.0.0",
  "description": "基于nestjs本地存储插件",
  "scripts": {
    "build": "node --max_old_space_size=4096 ./node_modules/gulp/bin/gulp.js build",
    "prepublish": "npm run snyk-protect; node --max_old_space_size=4096 ./node_modules/gulp/bin/gulp.js build",
    "publish": "node ./node_modules/lerna/bin/lerna.js publish -m \"chore(release): publish %s\"",
    "start": "node ./starter/index.js",
    "start:watch": "nodemon",
    "check": "tslint -p tsconfig.json -c tslint.json --type-check",
    "fix": "tslint -p tsconfig.json -c tslint.json --fix",
    "snyk-protect": "snyk protect"
  },
  "repository": {
    "type": "git",
    "url": "https://gitee.com/ibenchu/ext-local-store.git"
  },
  "author": "czy12345",
  "license": "Apache-2.0",
  "dependencies": {
    "@nestjs/common": "^5.0.0",
    "@nestjs/core": "^5.0.0",
    "@nestjs/graphql": "^3.0.0",
    "@nestjs/microservices": "^5.0.0",
    "@nestjs/testing": "^5.0.0",
    "@nestjs/typeorm": "^3.0.0",
    "@nestjs/websockets": "^5.0.0",
    "apollo-server-express": "^1.3.2",
    "formidable": "^1.1.1",
    "gm": "^1.23.1",
    "graphql": "^0.11.7",
    "graphql-tools": "^2.18.0",
    "mime": "^2.2.0",
    "mysql": "^2.15.0",
    "pg": "^7.4.1",
    "redis": "^2.7.1",
    "reflect-metadata": "^0.1.10",
    "request": "^2.83.0",
    "rxjs": "^6.1.0",
    "rxjs-compat": "^6.1.0",
    "sharp": "0.22.1",
    "typeorm": "^0.2.5",
    "typescript": "^2.5.2",
    "snyk": "^1.161.1"
  },
  "devDependencies": {
    "@notadd/gulp-typescript": "^4.0.2",
    "@types/node": "^8.0.28",
    "@types/sharp": "0.22.1",
    "gulp": "^3.9.1",
    "gulp-rename": "^1.2.2",
    "gulp-sequence": "^1.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-tslint": "^8.1.3",
    "gulp-watch": "^5.0.0",
    "lerna": "^2.9.1",
    "nodemon": "^1.12.1",
    "ts-node": "^3.3.0",
    "tslint": "^5.9.1"
  },
  "snyk": true
}
